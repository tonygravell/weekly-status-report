<table width="600" border="0" cellspacing="0" cellpadding="0">
  <tr>
    {{> feature-header feature-name="SUBSCRIPTION"
    icon="https://advancedkiosks.com/wp-content/uploads/2023/08/icon_subscription.png"}}
  </tr>
</table>
<table width="600">
  <tr>
    <td>
      {{#if subscriptionabouttoexpire}}
      <table width="100%" border="0" cellpadding="5">
        <tr>
          <td
            height="40"
            align="center"
            style="font-family: arial; background-color: red; font-size: 15px; font-weight: 600; color: #fff"
          >
            ATTENTION: Your Zamok Subscription Needs to be Renewed!
          </td>
        </tr>
      </table>
      {{else}}
      <table width="100%" border="0" cellpadding="5">
        <tr>
          <td
            height="40"
            align="center"
            style="font-family: arial; background-color: #328f00; font-size: 15px; font-weight: 600; color: #fff"
          >
            Your Zamok Subscription is in Good Standing!
          </td>
        </tr>
      </table>
      {{/if}}
    </td>
  </tr>
</table>

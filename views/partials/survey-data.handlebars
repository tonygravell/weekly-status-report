{{#if satisfactionmissing}} {{> feature-missing feature-name="SATISFACTION SURVEY"
icon="https://advancedkiosks.com/wp-content/uploads/2023/08/icon_survey.png"}} {{else}}
<table width="600" border="0" cellspacing="0" cellpadding="0">
  <tr>
    <td height="34" style="font-family: arial">
      <table
        width="600"
        border="0"
        cellspacing="0"
        cellpadding=""
        style="font-family: arial; margin-top: 20px; margin-bottom: 10px"
      >
        <tr>
          {{> feature-header feature-name="SATISFACTION SURVEY"
          icon="https://advancedkiosks.com/wp-content/uploads/2023/08/icon_survey.png"}}
          <td width="20%">
            <table width="155" border="0" cellspacing="0" style="font-family: arial; background-color: #ffb400">
              <tr>
                <td align="left" height="34" style="font-family: arial; font-size: 14px; text-align: center">
                  <strong>Total: </strong><strong>{{satisfactionresponsecount}}</strong>
                </td>
              </tr>
            </table>
          </td>
        </tr>
      </table>
    </td>
  </tr>
  <tr>
    <td height="48">
      <center>
        <table width="100%" border="0" cellspacing="5" cellpadding="10">
          <tr>
            <td width="64%" style="font-family: arial; background-color: #fff; font-size: 13px; font-weight: 600">
              SURVEY QUESTIONS
            </td>
            <td
              width="12%"
              align="center"
              style="font-family: arial; background-color: #fff; font-size: 13px; font-weight: 600; color: #328f00"
            >
              <img
                src="https://advancedkiosks.com/img/good.jpg"
                width="27"
                height="26"
                alt="Good"
                style="font-family: arial; width: 27px; height: 26px"
              />
            </td>
            <td
              width="12%"
              align="center"
              style="font-family: arial; background-color: #fff; font-size: 13px; font-weight: 600; color: #328f00"
            >
              <img
                src="https://advancedkiosks.com/img/medium.jpg"
                width="27"
                height="26"
                alt="Medium"
                style="font-family: arial; width: 27px; height: 26px"
              />
            </td>
            <td
              width="12%"
              align="center"
              style="font-family: arial; background-color: #fff; font-size: 13px; font-weight: 600; color: #328f00"
            >
              <img
                src="https://advancedkiosks.com/img/bad.jpg"
                width="27"
                height="26"
                alt="Bad"
                style="font-family: arial; width: 27px; height: 26px"
              />
            </td>
          </tr>
          <tr>
            <td style="font-family: arial; background-color: #d8d9db; font-size: 13px; font-weight: 600; padding: 10px">
              1. Did this kiosk help you achieve your goals?
            </td>
            <td
              align="center"
              style="font-family: arial; background-color: #f6f6f6; font-size: 13px; font-weight: 600; color: #328f00"
            >
              {{satisfactionhappycount}}
            </td>
            <td
              align="center"
              style="font-family: arial; background-color: #f6f6f6; font-size: 13px; font-weight: 600; color: #f5ac00"
            >
              {{satisfactionneutralcount}}
            </td>
            <td
              align="center"
              style="font-family: arial; background-color: #f6f6f6; font-size: 13px; font-weight: 600; color: #ff7800"
            >
              {{satisfactionsadcount}}
            </td>
          </tr>
          <tr>
            <td style="font-family: arial; background-color: #d8d9db; font-size: 13px; font-weight: 600; padding: 10px">
              2. Did you like using the interactive kiosk?
            </td>
            <td
              align="center"
              style="font-family: arial; background-color: #f6f6f6; font-size: 13px; font-weight: 600; color: #ff7800"
            >
              <span
                style="font-family: arial; background-color: #f6f6f6; font-size: 13px; font-weight: 600; color: #328f00"
                >{{satisfactionhappycount}}</span
              >
            </td>
            <td
              align="center"
              style="font-family: arial; background-color: #f6f6f6; font-size: 13px; font-weight: 600; color: #ff7800"
            >
              <span
                style="font-family: arial; background-color: #f6f6f6; font-size: 13px; font-weight: 600; color: #f5ac00"
                >{{satisfactionneutralcount}}</span
              >
            </td>
            <td
              align="center"
              style="font-family: arial; background-color: #f6f6f6; font-size: 13px; font-weight: 600; color: #ff7800"
            >
              {{satisfactionsadcount}}
            </td>
          </tr>
        </table>
      </center>
      <center></center>
    </td>
  </tr>
</table>
{{/if}}
